@article{defigueiredoStructureConductCorporate2001,
  title = {The {{Structure}} and {{Conduct}} of {{Corporate Lobbying}}: {{How Firms Lobby}} the {{Federal Communications Commission}}},
  shorttitle = {The {{Structure}} and {{Conduct}} of {{Corporate Lobbying}}},
  author = {family=Figueiredo, given=John M., prefix=de, useprefix=true and Tiller, Emerson H.},
  date = {2001},
  journaltitle = {Journal of Economics \& Management Strategy},
  volume = {10},
  number = {1},
  pages = {91--122},
  issn = {1530-9134},
  doi = {10.1111/j.1430-9134.2001.00091.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1430-9134.2001.00091.x},
  urldate = {2025-03-05},
  abstract = {This paper examines the amount and organization (individual vs. collective) of lobbying by firms in administrative agencies. It explores the power and limitations of the collective-action theories and transaction-cost theories in explaining lobbying. It introduces a dataset of over 900 lobbying contacts covering 101 issues at the Federal Communications Commission (FCC) in early 1998. It finds that the structure and conduct of large-firm lobbying at the FCC is consistent with the predictions of theories of transaction costs and the main results of theories of collective action. Small firms show little sensitivity to collective-action issues or transaction-cost issues in the organization of their lobbying, but they do lobby less when having to reveal proprietary information. In sum, large firms behave in a manner largely consistent with theoretical predictions, while small firms do not.},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/MyReseach_Related/Research_ideas/Lobbying/de Figueiredo and Tiller - 2001 - The Structure and Conduct of Corporate Lobbying How Firms Lobby the Federal Communications Commissi.pdf;/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/TIZD3ZP4/j.1430-9134.2001.00091.html}
}

@article{delaatCompaniesCommittedResponsible2021,
  title = {Companies {{Committed}} to {{Responsible AI}}: {{From Principles}} towards {{Implementation}} and {{Regulation}}?},
  shorttitle = {Companies {{Committed}} to {{Responsible AI}}},
  author = {De Laat, Paul B.},
  date = {2021-12},
  journaltitle = {Philosophy \& Technology},
  shortjournal = {Philos. Technol.},
  volume = {34},
  number = {4},
  pages = {1135--1193},
  issn = {2210-5433, 2210-5441},
  doi = {10.1007/s13347-021-00474-3},
  url = {https://link.springer.com/10.1007/s13347-021-00474-3},
  urldate = {2024-04-19},
  abstract = {The term ‘responsible AI’ has been coined to denote AI that is fair and non-biased, transparent and explainable, secure and safe, privacy-proof, accountable, and to the benefit of mankind. Since 2016, a great many organizations have pledged allegiance to such principles. Amongst them are 24 AI companies that did so by posting a commitment of the kind on their website and/or by joining the ‘Partnership on AI’. By means of a comprehensive web search, two questions are addressed by this study: (1) Did the signatory companies actually try to implement these principles in practice, and if so, how? (2) What are their views on the role of other societal actors in steering AI towards the stated principles (the issue of regulation)? It is concluded that some three of the largest amongst them have carried out valuable steps towards implementation, in particular by developing and open sourcing new software tools. To them, charges of mere ‘ethics washing’ do not apply. Moreover, some 10 companies from both the USA and Europe have publicly endorsed the position that apart from self-regulation, AI is in urgent need of governmental regulation. They mostly advocate focussing regulation on high-risk applications of AI, a policy which to them represents the sensible middle course between laissez-faire on the one hand and outright bans on technologies on the other. The future shaping of standards, ethical codes, and laws as a result of these regulatory efforts remains, of course, to be determined.},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/JC29VUIM/De Laat - 2021 - Companies Committed to Responsible AI From Princi.pdf}
}

