  isbn = {978-1-4503-8096-6},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/CBYG6HYD/Ehsan et al. - 2021 - Expanding Explainability Towards Social Transpare.pdf}
}

@article{eisenhardtBuildingTheoriesCase1989,
  title = {Building {{Theories}} from {{Case Study Research}}},
  author = {Eisenhardt, Kathleen M.},
  date = {1989},
