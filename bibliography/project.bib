@article{durandExpandingDomainStrategic2017,
  title = {The Expanding Domain of Strategic Management Research and the Quest for Integration},
  author = {Durand, Rodolphe and Grant, Robert M. and Madsen, Tammy L.},
  date = {2017-01},
  journaltitle = {Strategic Management Journal},
  shortjournal = {Strategic Management Journal},
  volume = {38},
  number = {1},
  pages = {4--16},
  issn = {0143-2095, 1097-0266},
  doi = {10.1002/smj.2607},
  url = {https://onlinelibrary.wiley.com/doi/10.1002/smj.2607},
  urldate = {2025-03-30},
  abstract = {Research summary                 : This special issue of                              Strategic Management Journal               was motivated by concern that the growing scope and diversity of the strategic management field creates the risk of incoherence and fragmentation and the belief that research reviews could contribute to synthesis and integration. In this introductory essay, we address the expanding domain of strategic management, consider where its boundaries lie, identify the forces engendering fragmentation, and discuss how this special issue—and research reviews in general—can assist convergence within the field of strategy. We conclude by addressing the potential for integration more broadly in relation to the theories we deploy, the phenomena we investigate, and cohesiveness of our scholarly community               .                                                          Managerial summary                 : The expanding domain of strategic management reflects the widening range of strategic issues that practising managers face. However, the fragmentation that has accompanied this broadening scope impedes the usefulness of strategic management research in guiding strategic decision making. We argue that reviews of strategic management research, such as those included within this special issue, can support the accumulation of an integrated, empirically-validated knowledge base which is essential to informing management practice                              . Copyright © 2016 John Wiley \& Sons, Ltd.},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/UO-phd/Spring-25/Strategy/Durand et al. - 2017 - The expanding domain of strategic management research and the quest for integration.pdf}
}

@incollection{eberhartFreedomJustAnother2022,
  title = {Freedom Is {{Just Another Word}} for {{Nothing Left}} to {{Lose}}: {{Entrepreneurialism}} and the {{Changing Nature}} of {{Employment Relations}}},
  shorttitle = {Freedom Is {{Just Another Word}} for {{Nothing Left}} to {{Lose}}},
  booktitle = {Research in the {{Sociology}} of {{Organizations}}},
  author = {Eberhart, Robert N. and Barley, Stephen and Nelson, Andrew},
  editor = {Eberhart, Robert N. and Lounsbury, Michael and Aldrich, Howard E.},
  date = {2022-09-22},
  pages = {13--41},
  publisher = {Emerald Publishing Limited},
  doi = {10.1108/S0733-558X20220000081002},
  url = {https://www.emerald.com/insight/content/doi/10.1108/S0733-558X20220000081002/full/html},
  urldate = {2025-02-27},
  abstract = {We explore the acceptance of new contingent work relationships in the United States to reveal an emergent entrepreneurial ideology. Our argument is that these new work relationships represent a new social order not situated in the conglomerates and labor unions of the past, but on a confluence of neoliberalism and individual action situated in the discourse of entrepreneurialism, employability, and free agency. This new employment relationship, which arose during the economic and social disruptions in the 1970s, defines who belongs inside an organization (and can take part in its benefits) and who must properly remain outside to fend for themselves. More generally, the fusing of entrepreneurship with neo-liberalism has altered not only how we work and where we work but also what we believe is appropriate work and what rewards should accompany it.},
  isbn = {978-1-80382-658-5 978-1-80382-657-8},
  langid = {english}
}

