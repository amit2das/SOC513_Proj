@online{cuiAgingScientistsSlowed2022,
  title = {Aging {{Scientists}} and {{Slowed Advance}}},
  author = {Cui, Haochuan and Wu, Lingfei and Evans, James A.},
  date = {2022-02-08},
  eprint = {2202.04044},
  eprinttype = {arXiv},
  eprintclass = {cs},
  doi = {10.48550/arXiv.2202.04044},
  url = {http://arxiv.org/abs/2202.04044},
  urldate = {2025-05-04},
  abstract = {What is the relationship between aging and the character of scientific advance? Prior research focuses on star scientists, their changing dates, and rates of breakthrough success through history. Analyzing more than 244 million scholars across 241 million articles over the last two centuries, we show that for all fields, periods, and impact levels, scientists research ideas and references age over time, their research is less likely to disrupt the state of science and more likely to criticize emerging work. Early success accelerates scientist aging; while changing institutions and fields and collaborating with young scientists slows it. These patterns aggregate within fields such that those with a higher proportion of older scientists experience a lower churn of ideas and more rapid individual aging, suggesting a universal link between aging, activity, and advance.},
  pubstate = {prepublished},
  keywords = {Computer Science - Digital Libraries},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/MyReseach_Related/Cui et al. - 2022 - Aging Scientists and Slowed Advance.pdf;/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/4MZEZTYI/2202.html}
