@article{freyPrivacyRegulationFirm2024,
  title = {Privacy Regulation and Firm Performance: {{Estimating}} the {{GDPR}} Effect Globally},
  shorttitle = {Privacy Regulation and Firm Performance},
  author = {Frey, Carl Benedikt and Presidente, Giorgio},
  date = {2024},
