@article{tripsasTechnologyIdentityInertia2009,
  title = {Technology, {{Identity}}, and {{Inertia Through}} the {{Lens}} of “{{The Digital Photography Company}}”},
  author = {Tripsas, Mary},
  date = {2009-04},
  journaltitle = {Organization Science},
  shortjournal = {Organization Science},
  volume = {20},
  number = {2},
  pages = {441--460},
  issn = {1047-7039, 1526-5455},
  doi = {10.1287/orsc.1080.0419},
  url = {https://pubsonline.informs.org/doi/10.1287/orsc.1080.0419},
  urldate = {2025-01-27},
  abstract = {Organizations often experience difficulty when pursuing new technology. Large bodies of research have examined the behavioral, social, and cognitive forces that underlie this phenomenon; however, the role of an organization's identity remains relatively unexplored. Identity comprises insider and outsider perceptions of what is core about an organization. An identity has associated with it a set of norms that represent shared beliefs about legitimate behavior for an organization with that identity. In this paper, technologies that deviate from the expectations associated with an organization's identity are labeled identity-challenging technologies. Based on a comprehensive field-based case study of the entire life history of a company, identity-challenging technologies are found to be difficult to capitalize on for two reasons. First, identity serves as a filter, such that organizational members notice and interpret external stimuli in a manner consistent with the identity. As a result, identity-challenging technological opportunities may be missed. Second, because identity becomes intertwined in the routines, procedures, and beliefs of both organizational and external constituents, explicit efforts to shift identity in order to accommodate identity-challenging technology are difficult. Given the disruptive nature of identity shifts, understanding whether technology is identity challenging is a critical consideration for managers pursuing new technology.},
  langid = {english}
}

@article{tsanevaChallengesGDPRCompliance2019,
  title = {Challenges of {{GDPR}} Compliance in Consumer Financing Companies},
  author = {Tsaneva, Monika},
