@article{woolleyMotivatingPersonalGrowth2022,
  title = {Motivating {{Personal Growth}} by {{Seeking Discomfort}}},
  author = {Woolley, Kaitlin and Fishbach, Ayelet},
  date = {2022-04},
  journaltitle = {Psychological Science},
  shortjournal = {Psychol Sci},
  volume = {33},
  number = {4},
  pages = {510--523},
  issn = {0956-7976, 1467-9280},
  doi = {10.1177/09567976211044685},
  url = {https://journals.sagepub.com/doi/10.1177/09567976211044685},
  urldate = {2025-01-08},
  abstract = {Achieving personal growth often requires experiencing discomfort. What if instead of tolerating discomfort (e.g., feeling awkward or uncomfortable), people actively sought it out? Because discomfort is usually experienced immediately and is easy to detect, we suggest that seeking discomfort as a signal of growth can increase motivation. Five experiments (total N = 2,163 adults) tested this prediction across various areas of personal growth: taking improvisation classes to increase self-confidence, engaging in expressive writing to process difficult emotions, becoming informed about the COVID-19 health crisis, opening oneself to opposing political viewpoints, and learning about gun violence. Across these areas of personal development, seeking discomfort as a signal of self-growth motivated engagement and increased perceived goal achievement relative to standard instructions. Consistent with our theorizing, results showed that these effects occurred only in areas of personal growth that cause immediate discomfort.},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/QCLGKZMT/Woolley and Fishbach - 2022 - Motivating Personal Growth by Seeking Discomfort.pdf}
}

