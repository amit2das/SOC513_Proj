  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/MyReseach_Related/Research_ideas/Choi et al. - 2015 - The Structure of Political Institutions and Effectiveness of Corporate Political Lobbying 2.pdf}
