@article{davidSystematicAssessmentEmpirical2004,
  title = {A Systematic Assessment of the Empirical Support for Transaction Cost Economics},
  author = {David, Robert J. and Han, Shin‐Kap},
  date = {2004-01},
  journaltitle = {Strategic Management Journal},
  shortjournal = {Strategic Management Journal},
  volume = {25},
