}

@article{helfatRenewingResourcebasedView2023,
  title = {Renewing the Resource-Based View: {{New}} Contexts, New Concepts, and New Methods},
  shorttitle = {Renewing the Resource-Based View},
  author = {Helfat, Constance E. and Kaul, Aseem and Ketchen Jr., David J. and Barney, Jay B. and Chatain, Olivier and Singh, Harbir},
