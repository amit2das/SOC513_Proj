@article{ahujaPERSPECTIVEExplainingInfluence2011,
  title = {{{PERSPECTIVE}}—{{Explaining Influence Rents}}: {{The Case}} for an {{Institutions-Based View}} of {{Strategy}}},
  shorttitle = {{{PERSPECTIVE}}—{{Explaining Influence Rents}}},
  author = {Ahuja, Gautam and Yayavaram, Sai},
  date = {2011-12},
  journaltitle = {Organization Science},
  volume = {22},
  number = {6},
