@article{suarezPerfectTimingDominant2015,
  title = {Perfect Timing? {{Dominant}} Category, Dominant Design, and the Window of Opportunity for Firm Entry},
  author = {Suarez, Fernando F. and Grodal, Stine and Gotsopoulos, Aleksios},
  date = {2015},
  journaltitle = {Strategic Management Journal},
  volume = {36},
