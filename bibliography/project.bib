  keywords = {Aggregate Productivity,Capacity Macroeconomics: Production Innovation and Invention: Processes and Incentives Empirical Studies of Economic Growth,Capital,Capital Total Factor and Multifactor Productivity,Cost,Cross-Country Output Convergence,Production}
}

