@article{eggersIncumbentAdaptationTechnological2018,
  title = {Incumbent {{Adaptation}} to {{Technological Change}}: {{The Past}}, {{Present}}, and {{Future}} of {{Research}} on {{Heterogeneous Incumbent Response}}},
  shorttitle = {Incumbent {{Adaptation}} to {{Technological Change}}},
