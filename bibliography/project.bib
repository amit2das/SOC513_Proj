@article{kristof-brownCONSEQUENCESINDIVIDUALSFIT2005,
  title = {{{CONSEQUENCES OF INDIVIDUALS}}' {{FIT AT WORK}}: {{A META}}‐{{ANALYSIS OF PERSON}}–{{JOB}}, {{PERSON}}–{{ORGANIZATION}}, {{PERSON}}–{{GROUP}}, {{AND PERSON}}–{{SUPERVISOR FIT}}},
