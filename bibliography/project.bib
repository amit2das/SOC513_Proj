@article{ibanezOperationalisingAIEthics2022,
  title = {Operationalising {{AI}} Ethics: How Are Companies Bridging the Gap between Practice and Principles? {{An}} Exploratory Study},
  shorttitle = {Operationalising {{AI}} Ethics},
  author = {Ibáñez, Javier Camacho and Olmeda, Mónica Villas},
  date = {2022-12},
  journaltitle = {AI \& SOCIETY},
  shortjournal = {AI \& Soc},
  volume = {37},
  number = {4},
  pages = {1663--1687},
  issn = {0951-5666, 1435-5655},
  doi = {10.1007/s00146-021-01267-0},
  url = {https://link.springer.com/10.1007/s00146-021-01267-0},
  urldate = {2024-04-19},
  abstract = {The rapid advancement and widespread deployment of Artificial Intelligence (AI) have raised concerns about its ethical and societal implications. Despite the growing emphasis on responsible AI development, there is limited understanding of industry’s engagement in responsible AI research—the critical examination of AI’s ethical, social, and legal dimensions. To address this gap, we conducted a large-scale analysis of over 6 million peer-reviewed articles and 22 million patent citations to quantify industry’s engagement in responsible AI research. Our findings—based on 3 distinct datasets—indicate that the majority of AI firms show limited or no engagement in this critical subfield of AI. Leading AI firms produce responsible AI research at a far lower rate than their conventional AI research and the output of leading academic institutions. Moreover, industry exhibits a narrower focus on responsible AI research, trailing academia in research topic diversity. Finally, a large-scale patent citation analysis reveals that industry patents rarely build on responsible AI papers, indicating a gap between responsible AI research and the commercialization of this increasingly ubiquitous technology. Overall, our results show that despite substantial resources and mounting external pressure, industry’s participation in responsible AI research remains limited in both depth and breadth, sharply contrasting its dominance in conventional AI research. Consequently, industry’s limited participation could inhibit its capacity to absorb academic knowledge, cultivate public trust, and, more importantly, address significant challenges such as mitigating AI-induced societal harms.},
  langid = {english},
  file = {/Users/amitdas/Library/CloudStorage/OneDrive-UniversityOfOregon/UO_PHD_Related/Zotero/storage/P3MFS3PM/Ibáñez and Olmeda - 2022 - Operationalising AI ethics how are companies brid.pdf}
}

