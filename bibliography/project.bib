@article{agarwalInvisibleGeniusesCould2020,
  title = {Invisible {{Geniuses}}: {{Could}} the {{Knowledge Frontier Advance Faster}}?},
  shorttitle = {Invisible {{Geniuses}}},
  author = {Agarwal, Ruchir and Gaule, Patrick},
  date = {2020-12-01},
  journaltitle = {American Economic Review: Insights},
  shortjournal = {American Economic Review: Insights},
  volume = {2},
  number = {4},
  pages = {409--424},
  issn = {2640-205X, 2640-2068},
  doi = {10.1257/aeri.20190457},
  url = {https://pubs.aeaweb.org/doi/10.1257/aeri.20190457},
  urldate = {2024-03-20},
  abstract = {A better understanding of the determinants of idea/knowledge production remains critical for long-run growth. Toward this end, this paper establishes two results using data from the International Mathematical Olympiad (IMO). First, individuals who excelled in teenage years are especially capable of advancing the knowledge frontier. Second, such talented individuals born in poorer countries are systematically less likely to engage in knowledge production. IMO participants from low-income countries produce 34 percent fewer publications and 56 percent fewer citations than equally talented rich-country counterparts. Policies to encourage talented youth to pursue scientific careers—especially those from poorer countries—could advance the knowledge frontier faster. (JEL D83, O30, O47)},
  langid = {english}
}

@article{agarwalLeveragingPrivateEnterprise2021,
  title = {Leveraging {{Private Enterprise}}: {{Incubation}} of {{New Industries}} to {{Address}} the {{Public Sector}}’s {{Mission-Oriented Grand Challenges}}},
  shorttitle = {Leveraging {{Private Enterprise}}},
  author = {Agarwal, Rajshree and Kim, Seojin and Moeen, Mahka},
  date = {2021-12},
  journaltitle = {Strategy Science},
  volume = {6},
  number = {4},
  pages = {385--411},
  publisher = {INFORMS},
  issn = {2333-2050},
  doi = {10.1287/stsc.2021.0137},
  url = {https://pubsonline.informs.org/doi/abs/10.1287/stsc.2021.0137},
  urldate = {2025-01-28},
  abstract = {We examine how mission-oriented grand challenges—formed to address the public sector’s unmet needs through development of new technologies and products for high potential impact—originate and catalyze industry incubation. Our analysis of six prominent cases identifies the incubation process, consisting of identification of unmet needs as a grand challenge, championing and articulation of a mission, leverage of private enterprise, and success or failure of the mission for subsequent industry emergence. The resulting conceptual model highlights key similarities and differences of industry incubation stemming from the public sector’s mission-oriented grand challenges relative to industries triggered by scientific discoveries or unmet user needs where the public sector is not as salient. The analysis reveals successful outcomes are associated with the public sector’s goal setting and carrying out “market functions” pertaining to selection, coordination, and knowledge sharing. We also provide cautions and caveats regarding fault lines that may arise in public-private partnerships.},
  keywords = {emerging industries,evolutionary economics,grand challenges,incubation stage,industry dynamics,technological change,technology and innovation management}
}

