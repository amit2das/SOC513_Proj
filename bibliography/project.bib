@article{chengCorporateSocialResponsibility2014,
  title = {Corporate Social Responsibility and Access to Finance},
  author = {Cheng, Beiting and Ioannou, Ioannis and Serafeim, George},
  date = {2014},
  journaltitle = {Strategic Management Journal},
  volume = {35},
  number = {1},
