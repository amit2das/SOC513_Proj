  booktitle = {Proceedings of the 2019 {{CHI Conference}} on {{Human Factors}} in {{Computing Systems}}},
  author = {Holstein, Kenneth and Wortman Vaughan, Jennifer and Daumé, Hal and Dudik, Miro and Wallach, Hanna},
